    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.2",
    "vite-plugin-eslint": "^1.8.1"
